{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request;Exception"
            },
            "enableLiveMetricsFilters": true
        },
        "logLevel": {
            "default": "Information",
            "Microsoft.EntityFrameworkCore.Database.Command": "Information",
            "Microsoft.EntityFrameworkCore": "Warning"
        }
    }
}